<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.spring.member.dao.EUserMapper">
    <insert id="insertEUser" parameterType="kr.spring.member.dto.EUserDTO">
        INSERT INTO EUSER (USER_NUM, EMPLOYEE_CODE, AUTHORITY)
        VALUES (#{userNum}, #{employeeCode}, #{authority})
    </insert>
</mapper>
