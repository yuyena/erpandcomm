<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.spring.member.dao.DepartmentMapper">

    <select id="selectDepartmentList" parameterType="map" resultType="kr.spring.member.vo.DepartmentVO">
        SELECT department_num, department_name, manager_num
        FROM department
        ORDER BY department_num ASC
    </select>

</mapper>
