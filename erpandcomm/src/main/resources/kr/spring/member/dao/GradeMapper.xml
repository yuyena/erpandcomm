<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.spring.member.dao.GradeMapper">

    <select id="selectGradeList" resultType="kr.spring.member.vo.GradeVO">
        SELECT GRADE_NUM, GRADE_NAME, GRADE_CODE
        FROM GRADE
        ORDER BY GRADE_NUM ASC
    </select>

</mapper>
